{
   "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
   "blocks": [
      {
         "segments": [
            {
               "background": "#196265",
               "background_templates": [
                  "{{ if gt .Code 0 }}#f1184c{{ end }}"
               ],
               "foreground": "#FFFFFF",
               "leading_diamond": "\ue0b6",
               "properties": {
                  "always_enabled": true
               },
               "style": "diamond",
               "template": "\ue23a {{ if gt .Code 0 }}\uf00d {{ reason .Code }}{{ else }}\uf42e{{ end }} ",
               "type": "status",
               "trailing_diamond": "\ue0c0"
            },
            {
               "background": "#FFC102",
               "foreground": "#00141A",
               "properties": {
                  "style": "roundrock",
                  "threshold": 0
               },
               "style": "diamond",
               "template": " {{ .FormattedMs }} ",
               "trailing_diamond": "\ue0c0",
               "type": "executiontime"
            },
            {
               "background": "#2AA198",
               "foreground": "#FFFFFF",
               "style": "diamond",
               "template": " {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB \ue266 ",
               "trailing_diamond": "\ue0c0",
               "type": "sysinfo"
            }
         ],
         "type": "rprompt"
      },
      {
         "alignment": "left",
         "newline": false,
         "segments": [
            {
               "type": "os",
               "style": "diamond",
               "foreground": "#0C212F",
               "background": "#27EEE0",
               "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} \ue0b1 \udb85\ude5d {{.HostName}} ",
               "trailing_diamond": "\ue0b0",
               "properties": {
                  "linux": "arc"
               }
            },
            {
               "background": "#FFC102",
               "foreground": "#0C212F",
               "leading_diamond": "<transparent,#FFC102>\ue0b0</>",
               "properties": {
                  "folder_icon": "\udb80\uddd8",
                  "folder_separator_icon": "<transparent> \ue0b1 </>",
                  "home_icon": "\ueb06",
                  "style": "agnoster_short"
               },
               "style": "diamond",
               "template": " {{ .Path }} ",
               "trailing_diamond": "\ue0b0",
               "type": "path"
            },
            {
               "background": "#2AA198",
               "background_templates": [
                  "{{ if or (.Working.Changed) (.Staging.Changed) }}#2AA198{{ end }}"
               ],
               "foreground": "#FFFFFF",
               "powerline_symbol": "\ue0b0",
               "properties": {
                  "branch_ahead_icon": "\u2191",
                  "branch_behind_icon": "\u2193",
                  "branch_gone": "\u2262",
                  "branch_icon": "\ue0a0 ",
                  "branch_identical_icon": "\u2261",
                  "cherry_pick_icon": "\u2713 ",
                  "commit_icon": "\u25b7 ",
                  "fetch_status": true,
                  "merge_icon": "\u25f4 ",
                  "no_commits_icon": "[no commits]",
                  "rebase_icon": "\u2c62 ",
                  "tag_icon": "\u25b6 "
               },
               "style": "powerline",
               "template": " on {{ .HEAD }}{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} {{ .Working.String }}{{ end }} ",
               "type": "git"
            },
            {
               "background": "#196265",
               "foreground": "#FFFFFF",
               "leading_diamond": "<transparent>\ue0b0 </>",
               "properties": {
                  "time_format": "3:04 PM"
               },
               "style": "diamond",
               "template": "\ue381 {{ .CurrentDate | date .Format }} ",
               "trailing_diamond": "\ue0b0",
               "type": "time"
            },
            {
               "background": "#012B36",
               "template": "<parentBackground>î‚°</parentBackground>",
               "properties": {
                  "always_enabled": true
               },
               "style": "diamond",
               "type": "status"
            }
         ],
         "type": "prompt"
      }
   ],
   "final_space": true,
   "version": 2
}

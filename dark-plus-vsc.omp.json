{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#222223>\u258c</>",
          "powerline_symbol": "\ue0b0",
          "background": "#222223",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#252527>\u258c</>",
          "powerline_symbol": "\ue0b0",
          "background": "#252527",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#28282B>\u258c</>",
          "powerline_symbol": "\ue0b0",
          "background": "#28282B",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "foreground": "#969698",
          "background": "#2D2D30",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b0",
          "template": " {{ .UserName }} ",
          "foreground": "#969698",
          "background": "#2D2D30",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#28282B></>",
          "powerline_symbol": "\ue0b0",
          "background": "#28282B",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": " ~",
            "style": "folder"
          },
          "template": " \uf115 {{ .Path }} ",
          "foreground": "#9FA1A4",
          "powerline_symbol": "\ue0b0",
          "background": "#2D2D30",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#2A2A2C></>",
          "powerline_symbol": "\ue0b0",
          "background": "#2A2A2C",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#272729></>",
          "powerline_symbol": "\ue0b0",
          "background": "#272729",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#242425></>",
          "powerline_symbol": "\ue0b0",
          "background": "#242425",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_ahead_icon": "\u2191",
            "branch_behind_icon": "\u2193",
            "branch_gone": "\u2262",
            "branch_icon": "\ue0a0 ",
            "branch_identical_icon": "\u2261",
            "cache_duration": "none",
            "cherry_pick_icon": "âœ“ ",
            "commit_icon": "\u25b7 ",
            "fetch_status": true,
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "[no commits]",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 "
          },
          "template": " {{ .HEAD }}{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} {{ .Working.String }}{{ end }} ",
          "foreground": "#BEBEBE",
          "powerline_symbol": "\ue0b0",
          "background": "#272729",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#272729{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#2B2B2C></>",
          "powerline_symbol": "\ue0b0",
          "background": "#2B2B2C",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "time_format": "15:04 PM"
          },
          "trailing_diamond": "\ue0b4",
          "template": " \ue641 {{ .CurrentDate | date .Format }}",
          "foreground": "#9FA1A4",
          "background": "#272729",
          "type": "time",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#272729>\ue0b0</>",
          "foreground": "#272729",
          "type": "text",
          "style": "plain"
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0c0",
          "template": "\ue23a {{ if gt .Code 0 }}\uf00d {{ reason .Code }}{{ else }}\uf42e{{ end }} ",
          "foreground": "#9FA1A4",
          "background": "#26282A",
          "type": "status",
          "style": "diamond",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#DC322F{{ end }}"
          ],
          "background_templates": [
            "{{ if gt .Code 0 }}#2C2C34{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "roundrock",
            "threshold": 0
          },
          "trailing_diamond": "\ue0c0",
          "template": " {{ .FormattedMs }} ",
          "foreground": "#9FA1A4",
          "background": "#2C2F30",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0c0",
          "template": " {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB \ue266 ",
          "foreground": "#9FA1A4",
          "background": "#333637",
          "type": "sysinfo",
          "style": "diamond"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}

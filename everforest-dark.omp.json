{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#333D42>\u258c</>",
          "powerline_symbol": "\ue0b0",
          "background": "#333D42",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#374046>\u258c</>",
          "powerline_symbol": "\ue0b0",
          "background": "#374046",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#3A454A>\u258c</>",
          "powerline_symbol": "\ue0b0",
          "background": "#3A454A",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "foreground": "#D3C6AA",
          "background": "#3D484D",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b0",
          "template": " {{ .UserName }} ",
          "foreground": "#D3C6AA",
          "background": "#3D484D",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#343D43></>",
          "powerline_symbol": "\ue0b0",
          "background": "#343D43",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "style": "folder"
          },
          "template": " \uf115  {{ .Path }} ",
          "foreground": "#A7C080",
          "powerline_symbol": "\ue0b0",
          "background": "#3D484D",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#3A454A></>",
          "powerline_symbol": "\ue0b0",
          "background": "#3A454A",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#374046></>",
          "powerline_symbol": "\ue0b0",
          "background": "#374046",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#333D42></>",
          "powerline_symbol": "\ue0b0",
          "background": "#333D42",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_ahead_icon": "\u2191",
            "branch_behind_icon": "\u2193",
            "branch_gone": "\u2262",
            "branch_icon": "\ue0a0 ",
            "branch_identical_icon": "\u2261",
            "cache_duration": "none",
            "cherry_pick_icon": "âœ“ ",
            "commit_icon": "\u25b7 ",
            "fetch_status": true,
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "[no commits]",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 "
          },
          "template": " {{ .HEAD }}{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} {{ .Working.String }}{{ end }} ",
          "foreground": "#7FBBB3",
          "powerline_symbol": "\ue0b0",
          "background": "#323B40",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#323B40{{ end }}"
          ]
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "time_format": "15:04 PM"
          },
          "trailing_diamond": "\ue0b4",
          "template": " \ue641 {{ .CurrentDate | date .Format }}",
          "foreground": "#D3C6AA",
          "background": "#3B4349",
          "type": "time",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#3B4349>\ue0b0</>",
          "foreground": "#3B4349",
          "type": "text",
          "style": "plain"
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0c0",
          "template": "{{ if gt .Code 0 }}\uf467{{ else }}\uf42e{{ end }} ",
          "foreground": "#7FBBB3",
          "background": "#434F55",
          "type": "status",
          "style": "diamond",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E67E80{{ end }}"
          ],
          "background_templates": [
            "{{ if gt .Code 0 }}#434F55{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "roundrock",
            "threshold": 0
          },
          "trailing_diamond": "\ue0c0",
          "template": " {{ .FormattedMs }} ",
          "foreground": "#D3C6AA",
          "background": "#374046",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0c0",
          "template": " {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB \ue266 ",
          "foreground": "#D3C6AA",
          "background": "#3D484D",
          "type": "sysinfo",
          "style": "diamond"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}

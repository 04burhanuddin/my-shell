{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": false,
      "segments": [
        {
          "background": "#343F44",
          "foreground": "#D3C6AA",
          "type": "os",
          "style": "plain",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}"
        },
        {
          "background": "#343F44",
          "foreground": "#D3C6AA",
          "style": "powerline",
          "template": " {{ .UserName }} ",
          "trailing_diamond": "\ue0b0",
          "type": "session"
        },
        {
          "background": "#343D43",
          "template": "<#343D43></>",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "type": "text"
        },
        {
          "background": "#3D484D",
          "foreground": "#A7C080",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "style": "folder"
          },
          "template": " \uf115  {{ .Path }} ",
          "type": "path"
        },
        {
          "template": "<#3A454A></>",
          "background": "#3A454A",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "type": "text"
        },
        {
          "background": "#374046",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": "<#374046></>",
          "type": "text"
        },
        {
          "background": "#333D42",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": "<#333D42></>",
          "type": "text"
        },
        {
          "background": "#323B40",
          "foreground": "#7FBBB3",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#323B40{{ end }}"
          ],
          "properties": {
            "branch_ahead_icon": "\u2191",
            "branch_behind_icon": "\u2193",
            "branch_gone": "\u2262",
            "branch_icon": "\ue0a0 ",
            "branch_identical_icon": "\u2261",
            "cherry_pick_icon": "\u2713 ",
            "commit_icon": "\u25b7 ",
            "fetch_status": true,
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "[no commits]",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 "
          },
          "template": " {{ .HEAD }}{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} {{ .Working.String }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#3B4349",
          "foreground": "#D3C6AA",
          "style": "diamond",
          "template": " \ue641 {{ .CurrentDate | date .Format }}",
          "trailing_diamond": "\ue0b4",
          "properties": {
            "always_enabled": true,
            "time_format": "15:04 PM"
          },
          "type": "time"
        },
        {
          "foreground": "#3B4349",
          "style": "plain",
          "template": "<#3B4349>\ue0b0</>",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "newline": false,
      "segments": [
        {
          "background": "#3A454A",
          "foreground": "#7FBBB3",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E67E80{{ end }}"
          ],
          "background_templates": [
            "{{ if gt .Code 0 }}#2C2C34{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "template": "\ue23a {{ if gt .Code 0 }}\uf00d {{ reason .Code }}{{ else }}\uf42e{{ end }} ",
          "type": "status",
          "trailing_diamond": "\ue0c0"
        },
        {
          "background": "#374046",
          "foreground": "#DBBC7F",
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "trailing_diamond": "\ue0c0",
          "type": "executiontime"
        },
        {
          "background": "#333D42",
          "foreground": "#A7C080",
          "style": "diamond",
          "template": " {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB \ue266 ",
          "trailing_diamond": "\ue0c0",
          "type": "sysinfo"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 2
}

{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#AADCFF",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#DC322F{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "foreground": "#113B3D",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "\ue23a {{ if gt .Code 0 }}\uf00d {{ reason .Code }}{{ else }}\uf42e{{ end }} ",
          "type": "status",
          "trailing_diamond": "\ue0c0"
        },
        {
          "background": "#2AA198",
          "foreground": "#113B3D",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "trailing_diamond": "\ue0c0",
          "type": "executiontime"
        },
        {
          "background": "#196265",
          "foreground": "#AADCFF",
          "style": "diamond",
          "template": " {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB \ue266 ",
          "trailing_diamond": "\ue0c0",
          "type": "sysinfo"
        }
      ],
      "type": "rprompt"
    },
    {
      "alignment": "left",
      "newline": false,
      "segments": [
        {
          "type": "os",
          "style": "powerline",
          "foreground": "#00141A",
          "background": "#6C71C4",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} \ue0b1 \udb85\ude5d {{ .HostName }} "
        },
        {
          "template": "<#9DA0ED></>",
          "background": "#9DA0ED",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "type": "text"
        },
        {
          "background": "#CBCFFF",
          "foreground": "#113B3D",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_icon": "\udb80\uddd8",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "\ueb06",
            "style": "agnoster_short"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "template": "<#48AEF5></>",
          "background": "#48AEF5",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "type": "text"
        },
        {
          "template": "<#278BD2></>",
          "background": "#278BD2",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "type": "text"
        },
        {
          "template": "<#1A6497></>",
          "background": "#1A6497",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "type": "text"
        },
        {
          "background": "#AADCFF",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#AADCFF{{ end }}"
          ],
          "foreground": "#113B3D",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_ahead_icon": "\u2191",
            "branch_behind_icon": "\u2193",
            "branch_gone": "\u2262",
            "branch_icon": "\ue0a0 ",
            "branch_identical_icon": "\u2261",
            "cherry_pick_icon": "\u2713 ",
            "commit_icon": "\u25b7 ",
            "fetch_status": true,
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "[no commits]",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 "
          },
          "style": "powerline",
          "template": " on {{ .HEAD }}{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} {{ .Working.String }}{{ end }} ",
          "type": "git"
        },
        {
          "template": "<#2AA198></>",
          "background": "#2AA198",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "type": "text"
        },
        {
          "background": "#196265",
          "foreground": "#AADCFF",
          "properties": {
            "time_format": "15:04 PM"
          },
          "style": "diamond",
          "template": " \ue381 {{ .CurrentDate | date .Format }}",
          "trailing_diamond": "\ue0b4",
          "type": "time"
        },
        {
          "template": "<#196265>\ue0b0</#>",
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}

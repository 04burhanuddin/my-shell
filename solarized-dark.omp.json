{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "foreground": "#CBCFFF",
          "background": "#163952",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b0",
          "template": " {{ .UserName }} ",
          "foreground": "#CBCFFF",
          "background": "#163952",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#1F425A></>",
          "powerline_symbol": "\ue0b0",
          "background": "#1F425A",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "style": "folder"
          },
          "template": " \uf115  {{ .Path }} ",
          "foreground": "#CBCFFF",
          "powerline_symbol": "\ue0b0",
          "background": "#294B5E",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#0E455C></>",
          "powerline_symbol": "\ue0b0",
          "background": "#0E455C",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#083E55></>",
          "powerline_symbol": "\ue0b0",
          "background": "#083E55",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#053649></>",
          "powerline_symbol": "\ue0b0",
          "background": "#053649",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_ahead_icon": "\u2191",
            "branch_behind_icon": "\u2193",
            "branch_gone": "\u2262",
            "branch_icon": "\ue0a0 ",
            "branch_identical_icon": "\u2261",
            "cache_duration": "none",
            "cherry_pick_icon": "âœ“ ",
            "commit_icon": "\u25b7 ",
            "fetch_status": true,
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "[no commits]",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 "
          },
          "template": " {{ .HEAD }}{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} {{ .Working.String }}{{ end }} ",
          "foreground": "#8FC0DF",
          "powerline_symbol": "\ue0b0",
          "background": "#224E5E",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#224E5E{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#084249></>",
          "powerline_symbol": "\ue0b0",
          "background": "#084249",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "time_format": "15:04 PM"
          },
          "trailing_diamond": "\ue0b4",
          "template": " \ue641 {{ .CurrentDate | date .Format }}",
          "foreground": "#2AA198",
          "background": "#0B484E",
          "type": "time",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#0B484E>\ue0b0</>",
          "foreground": "#05363F",
          "type": "text",
          "style": "plain"
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0c0",
          "template": "\ue23a {{ if gt .Code 0 }}\udb80\udd2c {{ reason .Code }}{{ else }}\uf42e{{ end }} ",
          "foreground": "#AADCFF",
          "background": "#224E5E",
          "type": "status",
          "style": "diamond",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#DC322F{{ end }}"
          ],
          "background_templates": [
            "{{ if gt .Code 0 }}#2C2C34{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "roundrock",
            "threshold": 0
          },
          "trailing_diamond": "\ue0c0",
          "template": " {{ .FormattedMs }} ",
          "foreground": "#27EDE0",
          "background": "#084249",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0c0",
          "template": " {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB \ue266 ",
          "foreground": "#2AA198",
          "background": "#05363F",
          "type": "sysinfo",
          "style": "diamond"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
